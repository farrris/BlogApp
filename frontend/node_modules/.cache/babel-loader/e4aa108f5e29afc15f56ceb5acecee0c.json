\">\r\n                <div class=\"modal-footer\">\r\n                    <button class=\"btn-close\" @click=\"closeModal\">\r\n                        Отмена\r\n                    </button>\r\n                    <button class='btn-update' @click=\"updateName\">\r\n                        Изменить\r\n                    </button>\r\n                </div>\r\n            </slot>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n    name: \"UpdateNameModalWindow\",\r\n    data() {\r\n        return {\r\n            show: false,\r\n            name: null,\r\n            aboba: 'kaka'\r\n        }\r\n    },\r\n    methods: {\r\n        closeModal() {\r\n            this.show = false\r\n        },\r\n        updateName() {\r\n            let name = {\r\n                xvalue: this.name\r\n            }\r\n            axios.put('http://127.0.0.1:8000/api/name/1', name).then(response => (console.log(response))).catch(error => (console.log(error)));\r\n            this.closeModal();\r\n            location.reload();\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.modal-shadow {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    min-height: 100%;\r\n    width: 100%;\r\n    background: rgba(0, 0, 0, 0.39);\r\n  }\r\n.modal {\r\n    background: #fff;\r\n    border-radius: 8px;\r\n    padding: 40px;\r\n    position: absolute;\r\n    top: 20%;\r\n    left: 40%;\r\n}\r\n\r\n.modal-title {\r\n    margin-bottom: 15%;\r\n}\r\n\r\n.modal-content {\r\n    text-align: center;\r\n}\r\n\r\n.modal-footer {\r\n    margin-top: 10%;\r\n    text-align: center;\r\n}\r\n\r\nbutton {\r\n    font-weight: bold;\r\n    padding: 5px;\r\n    background: none;\r\n    border: 2px solid black;\r\n    cursor: pointer;\r\n}\r\n\r\n.btn-update {\r\n    margin-left: 10%;\r\n}\r\ninput {\r\n    width: 80%;\r\n    padding: 2%;\r\n    margin-bottom: 10%;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module