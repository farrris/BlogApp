{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"CreateArticleModalWindow\",\n  data: function () {\n    return {\n      show: false,\n      title: null,\n      body: null\n    };\n  },\n  methods: {\n    closeModal: function () {\n      this.show = false;\n    },\n    getDate: function () {\n      let now = new Date();\n      let result = `${now.getDate}.${now.getMonth}.${now.getFullYear} ${now.getHours}:${now.getMinutes}`;\n      alert(result);\n    },\n    createArticle: function () {\n      let article = {\n        title: this.title,\n        body: this.body,\n        date: '22.22.22 22:22'\n      };\n      axios.post('http://127.0.0.1:8000/api/articles', article).then(response => console.log(response)).catch(error => console.log(error));\n      this.closeModal();\n    }\n  }\n};","map":{"version":3,"mappings":"AAgCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;AACXC,MAAI,EAAE,0BADK;AAEXC,MAAI,EAAE,YAAY;AACd,WAAO;AACHC,UAAI,EAAE,KADH;AAEHC,WAAK,EAAE,IAFJ;AAGHC,UAAI,EAAE;AAHH,KAAP;AAKH,GARU;AASXC,SAAO,EAAE;AACLC,cAAU,EAAE,YAAY;AACpB,WAAKJ,IAAL,GAAY,KAAZ;AACH,KAHI;AAKLK,WAAO,EAAE,YAAY;AACjB,UAAIC,GAAE,GAAI,IAAIC,IAAJ,EAAV;AACA,UAAIC,MAAK,GAAK,GAAEF,GAAG,CAACD,OAAQ,IAAGC,GAAG,CAACG,QAAS,IAAGH,GAAG,CAACI,WAAW,IAAIJ,GAAG,CAACK,QAAS,IAAGL,GAAG,CAACM,UAAW,EAAjG;AACAC,WAAK,CAACL,MAAD,CAAL;AACH,KATI;AAWLM,iBAAa,EAAE,YAAY;AACvB,UAAIC,OAAM,GAAI;AACVd,aAAK,EAAE,KAAKA,KADF;AAEVC,YAAI,EAAE,KAAKA,IAFD;AAGVc,YAAI,EAAE;AAHI,OAAd;AAKAnB,WAAK,CAACoB,IAAN,CAAW,oCAAX,EAAiDF,OAAjD,EAA0DG,IAA1D,CAA+DC,QAAO,IAAMC,OAAO,CAACC,GAAR,CAAYF,QAAZ,CAA5E,EAAoGG,KAApG,CAA0GC,KAAI,IAAMH,OAAO,CAACC,GAAR,CAAYE,KAAZ,CAApH;AACA,WAAKnB,UAAL;AACJ;AAnBK;AATE,CAAf","names":["axios","name","data","show","title","body","methods","closeModal","getDate","now","Date","result","getMonth","getFullYear","getHours","getMinutes","alert","createArticle","article","date","post","then","response","console","log","catch","error"],"sourceRoot":"","sources":["C:\\Users\\USER\\Desktop\\Programming\\TestA